<div id="c-note-versions">
  <div id="a-index">
    <h1>Note Changes</h1>

    <% if params.dig(:search, :post_id).present? || params.dig(:search, :note_id).present? %>
      <%= render "revert_listing" %>
    <% else %>
      <%= render "standard_listing" %>
    <% end %>

    <%= numbered_paginator(@note_versions) %>

    <%= render "notes/secondary_links" %>
  </div>
</div>
