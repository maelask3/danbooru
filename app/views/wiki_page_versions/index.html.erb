<%= render "wiki_pages/sidebar" %>

<% content_for(:content) do %>
  <h1>Wiki Page History</h1>

  <% if params.dig(:search, :wiki_page_id).present? %>
    <%= render "page_listing" %>
  <% else %>
    <%= render "global_listing" %>
  <% end %>

  <%= numbered_paginator(@wiki_page_versions) %>
<% end %>

